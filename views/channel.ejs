<!DOCTYPE html>
<html>
  <head>
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <ol>
      <% videos.forEach(video => { %>
      <li>
        <div>
          <img src="<%= getLocalThumbnail(video) %>" alt="<%= video.id %>" />
          <p><%= formatDuration(video.duration) %></p>
        </div>
        <a href="<%= `/watch?v=${video.id}` %>"><%= video.title %></a>
        <p><%= video.view_count.toLocaleString() %> views</p>
        <p><%= formatDate(video.upload_date) %></p>
      </li>
      <% }); %>
    </ol>
  </body>
</html>
